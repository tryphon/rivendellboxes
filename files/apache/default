<VirtualHost *:80>      
  ServerName rivendellbox.local
  ServerAdmin support@tryphon.eu

  DNSSDServiceName "Tryphon RivendellBox"

  DocumentRoot /var/www

  LogLevel warn

  # Use only syslog
  ErrorLog syslog
  CustomLog "|/usr/bin/logger -p local1.info -t apache2/access" combined
</VirtualHost>
